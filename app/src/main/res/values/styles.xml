<resources xmlns:tools="http://schemas.android.com/tools">

    <style name="ActivityListTheme" parent="Theme.MaterialComponents.Light.NoActionBar">
        <item name="colorPrimary">@color/white</item>
        <item name="colorAccent">@color/white</item>
        <item name="colorPrimaryDark">@color/white</item>
        <item name="windowActionBar">false</item>
        <item name="android:windowNoTitle">true</item>
        <item name="windowActionModeOverlay">true</item>
        <item name="android:includeFontPadding">false</item>
        <item name="actionModeBackground">@color/gray_757575</item>
        <item name="alertDialogTheme">@style/MyAlertDialogStyle</item>
        <item name="android:windowLightStatusBar" tools:targetApi="m">true</item>
        <!--<item name="android:toolbarStyle">@style/AppThemeToolbar</item>
        <item name="toolbarStyle">@style/AppThemeToolbar</item>-->
        <item name="colorControlNormal">@color/white</item>
        <item name="bottomSheetDialogTheme">@style/BottomSheetTheme</item>
        <item name="android:textColorSecondary">@color/white</item>
        <item name="materialButtonStyle">@style/MaterialButtonStyle</item>

    </style>

    <style name="RatingBar" parent="Theme.AppCompat">
        <item name="colorControlNormal">@color/white</item>
        <item name="colorControlActivated">@color/yellow_circle_bg</item>
    </style>

    <style name="checkboxstyle" parent="Base.Theme.AppCompat">
        <item name="colorPrimary">@color/blue</item>
        <item name="colorAccent">@color/blue</item>
        <item name="colorControlNormal">@color/gray_999</item>
        <item name="android:textColorSecondary">@color/gray_999</item>

    </style>

    <style name="FloatingEditTextDialog" parent="Theme.MaterialComponents.Light.BottomSheetDialog">
        <item name="android:windowIsFloating">false</item>
        <item name="android:windowSoftInputMode">adjustResize</item>
    </style>

    <style name="AppTheme.SeekBar" parent="Theme.MaterialComponents.Light.NoActionBar">
        <item name="colorPrimary">@color/teacherheader</item>
        <item name="colorPrimaryDark">@color/teacherheader</item>
        <item name="colorAccent">@color/teacherheader</item>
    </style>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.MaterialComponents.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/white</item>
        <item name="android:includeFontPadding">false</item>
        <item name="colorPrimaryDark">@color/white</item>
        <item name="alertDialogTheme">@style/MyAlertDialogStyle</item>
        <item name="colorAccent">@color/white</item>
        <item name="android:textColor">@android:color/black</item>
        <item name="android:windowLightStatusBar" tools:targetApi="m">true</item>
        <item name="android:windowAnimationStyle">@null</item>
        <item name="bottomSheetDialogTheme">@style/BottomSheetTheme</item>
        <item name="materialButtonStyle">@style/MaterialButtonStyle</item>
    </style>


    <style name="homescreen" parent="Theme.MaterialComponents.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/color_ffb912</item>
        <item name="android:includeFontPadding">false</item>
        <item name="colorPrimaryDark">@color/white</item>
        <item name="alertDialogTheme">@style/MyAlertDialogStyle</item>
        <item name="colorAccent">@color/color_ffb912</item>
        <item name="android:textColor">@android:color/black</item>
        <item name="android:windowLightStatusBar" tools:targetApi="m">true</item>
        <item name="android:windowAnimationStyle">@null</item>
        <item name="bottomSheetDialogTheme">@style/BottomSheetTheme</item>
        <item name="materialButtonStyle">@style/MaterialButtonStyle</item>
    </style>

    <style name="MaterialButtonStyle" parent="TextAppearance.MaterialComponents.Button">
        <item name="android:background">@android:color/transparent</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="YourLightTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="android:windowActionBar">false</item>
        <item name="android:windowFullscreen">false</item>
        <item name="windowNoTitle">true</item>
        <item name="colorPrimary">@color/white</item>
        <item name="android:windowLightStatusBar" tools:targetApi="m">true</item>
        <item name="android:includeFontPadding">false</item>
        <item name="colorPrimaryDark">@android:color/transparent</item>
        <item name="alertDialogTheme">@style/MyAlertDialogStyle</item>
        <item name="colorAccent">@color/white</item>
        <item name="android:textColor">@android:color/black</item>
        <item name="android:windowAnimationStyle">@null</item>
        <item name="bottomSheetDialogTheme">@style/BottomSheetTheme</item>
        <item name="materialButtonStyle">@style/MaterialButtonStyle</item>
        <item name="toolbarStyle">@style/CustomToolBarStyle</item>
        <!--
        <item name="selectableItemBackground">?android:selectableItemBackground</item>
        <item name="selectableItemBackgroundBorderless">?android:selectableItemBackground</item>
           <item name="toolbarStyle">@style/CustomToolBarStyle</item>-->
    </style>

    <style name="CustomToolBarStyle" parent="@style/Widget.AppCompat.Toolbar">
        <item name="titleTextColor">@color/black</item>
        <item name="navigationIcon">@drawable/back_button</item>

    </style>

    <style name="CollapsingToolbarExpanded" parent="TextAppearance.Design.CollapsingToolbar.Expanded">
        <item name="android:textColor">@color/color_002366</item>
        <item name="android:textSize">@dimen/_25sdp</item>
    </style>

    <style name="CollapsingToolbarCollapse" parent="TextAppearance.AppCompat.Title">
        <item name="android:textColor">@color/color_002366</item>
        <item name="android:textSize">@dimen/_17sdp</item>
    </style>

    <style name="CustomToolBarStyleWithouBackIcon" parent="@style/Widget.AppCompat.Toolbar">
        <item name="titleTextColor">@color/black</item>
        <item name="collapsedTitleGravity">center_horizontal</item>
        <item name="android:layout_gravity">center_vertical</item>
    </style>


    <style name="AppThemeDark" parent="Theme.MaterialComponents.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/white</item>
        <item name="colorPrimaryDark">@color/white</item>
        <item name="colorAccent">@color/white</item>
        <item name="android:includeFontPadding">false</item>
        <item name="alertDialogTheme">@style/MyAlertDialogStyle</item>
        <item name="android:windowLightStatusBar" tools:targetApi="m">true</item>
        <item name="bottomSheetDialogTheme">@style/BottomSheetTheme</item>
        <item name="android:textColor">@android:color/black</item>
        <item name="android:windowBackground">@android:color/black</item>
        <item name="materialButtonStyle">@style/MaterialButtonStyle</item>
    </style>

    <style name="AppFullScreenMaterial" parent="android:Theme.Material">
        <item name="android:colorPrimary">#5665bb</item>
        <item name="android:colorPrimaryDark">#41456b</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowActionBar">false</item>
        <item name="android:windowFullscreen">true</item>
        <item name="bottomSheetDialogTheme">@style/BottomSheetTheme</item>
        <item name="alertDialogTheme">@style/MyAlertDialogStyle</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="materialButtonStyle">@style/MaterialButtonStyle</item>

    </style>

    <style name="AppFullScreen" parent="Theme.MaterialComponents.Light.NoActionBar">
        <item name="colorPrimary">@color/white</item>
        <item name="colorPrimaryDark">@color/white</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowActionBar">false</item>
        <item name="android:windowFullscreen">true</item>
        <item name="bottomSheetDialogTheme">@style/BottomSheetTheme</item>
        <item name="alertDialogTheme">@style/MyAlertDialogStyle</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="materialButtonStyle">@style/MaterialButtonStyle</item>
    </style>

    <style name="NewUI.TextInputLayout" parent="@style/TextAppearance.AppCompat">
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">@dimen/_12sdp</item>
        <item name="android:includeFontPadding">false</item>
    </style>

    <style name="TextAppearence.App.TextInputLayout" parent="@android:style/TextAppearance">
        <item name="android:textColor">@color/color_ffb912</item>
        <item name="android:textSize">@dimen/_11sdp</item>
        <item name="android:includeFontPadding">false</item>
        <item name="colorPrimary">@color/color_ffb912</item>
    </style>


    <style name="TextAppearence.App.TextInputLayoutLarge" parent="@android:style/TextAppearance">
        <item name="android:textSize">@dimen/_14sdp</item>
        <item name="android:includeFontPadding">false</item>
    </style>

    <style name="TextAppearence.Support.TextInputLayout" parent="@android:style/TextAppearance">
        <item name="android:textColor">@color/black</item>
        <item name="android:includeFontPadding">false</item>
        <item name="android:textSize">@dimen/_12sdp</item>
    </style>


    <style name="EditTextAppearence" parent="@android:style/TextAppearance">
        <item name="android:textColor">@android:color/black</item>
        <item name="android:textSize">@dimen/_15sdp</item>
        <item name="android:includeFontPadding">false</item>
        <item name="android:singleLine">true</item>
    </style>

    <style name="ActivitySlideDownAnimation">
        <item name="android:windowEnterAnimation">@null</item>
        <item name="android:windowExitAnimation">@anim/back_bottom</item>
        <item name="android:interpolator">@android:interpolator/linear</item>
    </style>

    <style name="AppCompatButtonStyle" parent="TextAppearance.AppCompat.Button">
        <item name="android:background">@android:color/transparent</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="MyAlertDialogStyle" parent="Theme.AppCompat.Light.Dialog">
        <!-- Used for the buttons -->
        <item name="colorAccent">@color/white</item>
        <item name="colorPrimary">@color/white</item>
        <item name="colorPrimaryDark">@color/white</item>
        <item name="android:includeFontPadding">false</item>
        <item name="buttonStyle">@style/AppCompatButtonStyle</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="CalenderBottomSheet" parent="Theme.AppCompat.Light.Dialog">
        <!-- Used for the buttons -->
        <item name="colorPrimary">@color/white</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/white</item>
        <item name="buttonStyle">@style/AppCompatButtonStyle</item>
        <item name="android:windowTranslucentNavigation">true</item>
        <item name="android:windowBackground">@android:color/transparent</item>
    </style>

    <style name="reseDialogtheme">
        <item name="android:windowBackground">@color/resetpop_backgroundcolor</item>
        <item name="android:windowIsTranslucent">false</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:includeFontPadding">false</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:backgroundDimEnabled">true</item>
    </style>

    <style name="BottomSheetTheme" parent="Theme.MaterialComponents.Light.BottomSheetDialog">
        <item name="android:windowNoTitle">true</item>
        <item name="colorPrimary">@color/white</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="android:windowTranslucentNavigation">true</item>
        <item name="android:windowBackground">@android:color/transparent</item>
    </style>

    <style name="Transparent" parent="Theme.MaterialComponents.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="android:textColor">@android:color/black</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:backgroundDimEnabled">true</item>
        <item name="android:includeFontPadding">false</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="bottomSheetDialogTheme">@style/BottomSheetTheme</item>
        <item name="alertDialogTheme">@style/MyAlertDialogStyle</item>
        <item name="android:windowIsFloating">false</item>
        <item name="android:windowAnimationStyle">@null</item>
    </style>

    <style name="Calender.TextAppearence" parent="@android:style/TextAppearance">
        <item name="android:textSize">@dimen/_13sdp</item>
    </style>


    <style name="DialogFadeAnimation">
        <item name="android:windowEnterAnimation">@android:anim/fade_in</item>
        <item name="android:windowExitAnimation">@android:anim/fade_out</item>
    </style>


    <style name="AppTheme.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

    <style name="SplashScreenTheme" parent="Theme.AppCompat.NoActionBar">
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowActionBar">false</item>
        <item name="android:windowFullscreen">true</item>
        <item name="android:windowBackground">@drawable/launcher_screen_with_logo</item>
    </style>

    <style name="progressBarTheme">
        <item name="colorAccent">@color/dark_yellow</item>
    </style>

    <style name="TextLabel" parent="TextAppearance.AppCompat">
        <!-- Hint color and label color in FALSE state -->
        <item name="android:textColorHint">@color/color_ffb912</item>
        <item name="android:textSize">20sp</item>
        <!-- Label color in TRUE state and bar color FALSE and TRUE State -->
        <item name="colorAccent">@color/color_ffb912</item>
        <item name="colorControlNormal">@color/color_ffb912</item>
        <item name="colorControlActivated">@color/color_ffb912</item>
    </style>

</resources>
